{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2019",
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDirs": [
      "src",
      "tests"
    ],
    "baseUrl": ".",
    "paths": {
      "@/tests/*": [
        "tests/*"
      ],
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "tests",
    "src/db/config/knexfile.ts"
  ],
  "exclude": []
}