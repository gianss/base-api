{
  "name": "nutri",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/main/server.js",
    "start:dev": "nodemon",
    "debug": "nodemon -L --watch  --inspect=0.0.0.0:9222 --nolazy",
    "knex": "knex --knexfile ./src/db/config/knexfile.js",
    "build": "tsc --project tsconfig-build.json && tsconfig-replace-paths -p tsconfig-build.json -s ./src -o ./dist",
    "build:watch": "tsc --project tsconfig-build.json -w && tsconfig-replace-paths -p tsconfig-build.json -s ./src -o ./dist",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "eslint": "^7.28.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "knex": "^3.0.1",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.15",
    "sucrase": "^3.20.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.2.7",
    "fs": "^0.0.1-security",
    "handy-storage": "^2.1.6",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.6.0"
  }
}